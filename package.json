{
  "name": "kan-budgit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "deploy": "yarn data:seeds1 && yarn data:seeds2 && vue-cli-service build",
    "lint": "vue-cli-service lint src/ import/",
    "format": "vue-cli-service lint --fix src/ import/",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test:unit": "vue-cli-service test:unit --passWithNoTests --coverage && cat ./coverage/lcov.info | codacy-coverage --language typescript --token $CODACY_PROJECT_TOKEN",
    "test:unit:watch": "vue-cli-service test:unit --watch",
    "data:clean": "rm -rf ./public/data/accounts/* && rm -rf ./public/data/boards/*",
    "data:import": "ts-node --project ./tsconfig-import.json -r tsconfig-paths/register ./import/index.ts",
    "data:seeds1": "BUDGIT_DATA_PATH=./data/seeds1/ yarn data:import",
    "data:seeds2": "BUDGIT_DATA_PATH=./data/seeds2/ yarn data:import"
  },
  "dependencies": {
    "@types/node": "^12.12.14",
    "@types/rmfr": "^2.0.0",
    "core-js": "^3.4.5",
    "fs": "^0.0.1-security",
    "moment": "^2.24.0",
    "normalize-scss": "^7.0.1",
    "register-service-worker": "^1.6.2",
    "rmfr": "^2.0.0",
    "ts-node": "^8.5.4",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-e2e-cypress": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-plugin-pwa": "^4.1.1",
    "@vue/cli-plugin-typescript": "^4.1.1",
    "@vue/cli-plugin-unit-jest": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "codacy-coverage": "^3.4.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^6.0.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "ts-jest": "^24.2.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10"
  }
}

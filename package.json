{
  "name": "kan-budgit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "deploy": "yarn data:seeds1 && yarn data:seeds2 && vue-cli-service build",
    "lint": "vue-cli-service lint src/ import/",
    "format": "vue-cli-service lint --fix src/ import/",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test:unit": "vue-cli-service test:unit --passWithNoTests --coverage && cat ./coverage/lcov.info | codacy-coverage --language typescript --token $CODACY_PROJECT_TOKEN",
    "test:unit:watch": "vue-cli-service test:unit --watch",
    "data:clean": "rm -rf ./public/data/accounts/* && rm -rf ./public/data/boards/*",
    "data:import": "ts-node --project ./tsconfig-import.json -r tsconfig-paths/register ./import/index.ts",
    "data:seeds1": "BUDGIT_DATA_PATH=./data/seeds1/ yarn data:import",
    "data:seeds2": "BUDGIT_DATA_PATH=./data/seeds2/ yarn data:import"
  },
  "dependencies": {
    "@types/fs-extra": "8.0.1",
    "@types/node": "13.1.4",
    "@types/rmfr": "2.0.0",
    "core-js": "3.6.2",
    "fs-extra": "8.1.0",
    "moment": "2.24.0",
    "normalize-scss": "7.0.1",
    "register-service-worker": "1.6.2",
    "rmfr": "2.0.0",
    "ts-node": "8.5.4",
    "vue": "2.6.11",
    "vue-class-component": "7.1.0",
    "vue-property-decorator": "8.3.0",
    "vue-router": "3.1.3",
    "vuex": "3.1.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.25",
    "@vue/cli-plugin-babel": "4.1.2",
    "@vue/cli-plugin-e2e-cypress": "4.1.2",
    "@vue/cli-plugin-eslint": "4.1.2",
    "@vue/cli-plugin-pwa": "4.1.2",
    "@vue/cli-plugin-typescript": "4.1.2",
    "@vue/cli-plugin-unit-jest": "4.1.2",
    "@vue/cli-service": "4.1.2",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.30",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "codacy-coverage": "3.4.0",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-vue": "6.1.1",
    "node-sass": "4.13.0",
    "prettier": "1.19.1",
    "sass-loader": "8.0.0",
    "ts-jest": "24.2.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.4",
    "vue-template-compiler": "2.6.11"
  }
}
